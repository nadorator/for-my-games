{
  "name": "nadorator/formygames",
  "type": "project",
  "minimum-stability": "alpha",
  "require": {
    "php": ">=8.2",
    "monolog/monolog": "2.10.0",
    "phalcon/cphalcon": "v5.8.0",
    "phpmailer/phpmailer": "~6.0",
    "phalcon/zephir": "0.18.0",
    "php-amqplib/php-amqplib": "2.7.*",
    "mobiledetect/mobiledetectlib": "^2.8",
    "giggsey/libphonenumber-for-php": "*",
    "google/recaptcha": "~1.1",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/console": "^v5.4.47",
    "ovh/ovh": "^2.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-xml": "*",
    "ext-mysqli": "*",
    "ext-simplexml": "*",
    "ext-gd": "*",
    "ext-soap": "*",
    "ext-zlib": "*",
    "ext-imagick": "*",
    "ext-zip": "*",
    "ext-intl": "*",
    "ext-exif": "*",
    "ext-xdebug": "*",
    "ext-fileinfo": "*",
    "ext-mbstring": "*",
    "econea/nusoap": "~0.9.5.1",
    "calcinai/php-imagick": "^0.1.2",
    "ext-openssl": "*",
    "ext-posix": "*",
    "ext-iconv": "*",
    "orhanerday/open-ai": "^5.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "pestphp/pest": "^1.23"
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest",
    "test:parallel": "./vendor/bin/pest --parallel",
    "phpcs": "phpcs --standard=PSR12",
    "phpcbf": "phpcbf --standard=PSR12"
  }
}
